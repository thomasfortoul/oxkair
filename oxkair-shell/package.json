{
  "name": "oxkair-shell",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node server.js",
    "lint": "npx next lint",
    "test": "npx jest --forceExit --detectOpenHandles",
    "test:db": "npx jest app/api/__tests__ --forceExit --detectOpenHandles",
    "test:types": "node __tests__/run-type-tests.js",
    "test:types:agent": "npx jest __tests__/types/agent-type-consistency.test.js --config jest.types.config.json --forceExit",
    "test:types:ui": "npx jest __tests__/types/ui-type-consistency.test.js --config jest.types.config.json --forceExit",
    "test:types:data": "npx jest __tests__/types/data-processing-type-consistency.test.js --config jest.types.config.json --forceExit",
    "test:types:cross": "npx jest __tests__/types/cross-agent-type-consistency.test.js --config jest.types.config.json --forceExit",
    "test:types:api": "npx jest __tests__/types/api-type-consistency.test.js --config jest.types.config.json --forceExit",
    "test:types:definitions": "npx jest __tests__/types/type-definition-validation.test.js --config jest.types.config.json --forceExit",
    "test:types:integration": "npx jest __tests__/types/integration-type-consistency.test.js --config jest.types.config.json --forceExit",
    "test:all": "npm run test && npm run test:types",
    "validate:test-setup": "node __tests__/validate-test-setup.js",
    "setup-tests": "node scripts/setup_tests.js",
    "setup-test-files": "node scripts/setup_tests.js --generate-files",
    "run-case-tests": "./run_tests.sh",
    "compare-results": "node scripts/compare_results.js",
    "full-case-test": "npm run setup-test-files && npm run run-case-tests && npm run compare-results"
  },
